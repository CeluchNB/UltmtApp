{
  "name": "ultmtapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:e2e": "NODE_ENV=e2e react-native start --reset-cache",
    "test:e2e": "./e2e/run-e2e-tests.sh",
    "test": "TZ=UTC jest --collectCoverage",
    "test-ci": "TZ=UTC jest --forceExit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "updateperm": "react-native setup-ios-permissions && cd ios && pod install && cd .."
  },
  "reactNativePermissionsIOS": [
    "AppTrackingTransparency"
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/checkbox": "^0.5.16",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-picker/picker": "^2.5.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/material-bottom-tabs": "^6.2.16",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@realm/react": "^0.6.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.14",
    "@types/validator": "^13.11.1",
    "axios": "1.5.0",
    "dayjs": "^1.11.9",
    "jwt-decode": "^3.1.2",
    "password-validator": "^5.3.0",
    "react": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-native": "0.72.4",
    "react-native-date-picker": "^4.2.14",
    "react-native-dotenv": "^3.4.9",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-get-random-values": "^1.9.0",
    "react-native-google-mobile-ads": "^12.1.1",
    "react-native-pager-view": "^6.2.1",
    "react-native-paper": "^5.10.4",
    "react-native-permissions": "^3.9.0",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^10.0.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "realm": "^11.10.1",
    "socket.io-client": "^4.7.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@babel/runtime": "^7.22.15",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "0.72.9",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/react-native": "^3.0.2",
    "@types/detox": "^18.1.0",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/react": "^18.2.1",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-jest": "^29.6.4",
    "detox": "^20.11.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ft-flow": "^3.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "jest": "^29.6.4",
    "jsonwebtoken": "^9.0.2",
    "metro-react-native-babel-preset": "^0.76.8",
    "mockdate": "^3.0.5",
    "pod-install": "^0.1.39",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": ".",
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.ts",
      "<rootDir>/__tests__/**/*-test.tsx"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-iphone-x-helper|react-native-vector-icons|@realm/react)/)"
    ]
  }
}
