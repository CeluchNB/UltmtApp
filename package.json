{
  "name": "ultmtapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:e2e": "NODE_ENV=e2e react-native start --reset-cache",
    "test:e2e": "./e2e/run-e2e-tests.sh",
    "test": "TZ=UTC jest --collectCoverage",
    "test-ci": "TZ=UTC jest --runInBand --forceExit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "updateperm": "react-native setup-ios-permissions && cd ios && pod install && cd .."
  },
  "reactNativePermissionsIOS": [
    "AppTrackingTransparency"
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-clipboard/clipboard": "^1.15.0",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.10.2",
    "@react-navigation/native": "7.0.14",
    "@react-navigation/native-stack": "7.2.0",
    "@realm/react": "^0.11.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@types/lodash": "^4.17.14",
    "@types/react-native-dotenv": "^0.2.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/validator": "^13.12.2",
    "axios": "1.7.9",
    "dayjs": "^1.11.13",
    "eventemitter3": "^5.0.1",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "password-validator": "^5.3.0",
    "react": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.6",
    "react-native-date-picker": "^5.0.8",
    "react-native-dotenv": "^3.4.11",
    "react-native-element-dropdown": "^2.12.2",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-gifted-charts": "^1.4.53",
    "react-native-google-mobile-ads": "14.7.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-pager-view": "^6.6.1",
    "react-native-paper": "5.12.5",
    "react-native-permissions": "^5.2.2",
    "react-native-responsive-linechart": "^5.7.1",
    "react-native-safe-area-context": "5.0.0",
    "react-native-screens": "4.4.0",
    "react-native-svg": "15.10.1",
    "react-native-tab-view": "^4.0.5",
    "react-native-vector-icons": "^10.2.0",
    "react-query": "^3.39.3",
    "react-redux": "8.1.3",
    "realm": "^12.8.1",
    "socket.io-client": "^4.8.1",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@faker-js/faker": "^8.4.1",
    "@react-native/babel-preset": "0.76.6",
    "@react-native/eslint-config": "^0.76.6",
    "@react-native/metro-config": "0.76.6",
    "@react-native/typescript-config": "0.76.6",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-native": "^12.6.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/detox": "^18.1.0",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/react": "^18.3.12",
    "@types/react-test-renderer": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "babel-jest": "^29.6.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.11.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ft-flow": "^3.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "fishery": "^2.2.2",
    "jest": "^29.6.4",
    "jsonwebtoken": "^9.0.2",
    "mockdate": "^3.0.5",
    "pod-install": "^0.3.2",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": ".",
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.ts",
      "<rootDir>/__tests__/**/*.test.tsx",
      "<rootDir>/__tests__/**/*-test.tsx"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "@ultmt-app/(.*)": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-iphone-x-helper|react-native-vector-icons|@realm/react|@realm/fetch)/)"
    ]
  },
  "packageManager": "yarn@1.22.22"
}
