{
  "name": "ultmtapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:e2e": "NODE_ENV=e2e react-native start",
    "test:e2e": "./e2e/run-e2e-tests.sh",
    "test": "TZ=UTC jest --collectCoverage",
    "test-ci": "TZ=UTC jest --forceExit",
